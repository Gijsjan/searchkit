  },
  "devDependencies": {
    "@types/enzyme": "^2.8.11",
    "@types/jest": "^21.1.2",
    "@types/react": "^15.0.0",
    "@types/react-dom": "^15.0.0",
    "@types/react-router": "^2.0.51",
    "@types/selenium-webdriver": "^2.53.39",
    "@types/sinon": "^1.16.34",
